{
  "name": "mlflow-js",
  "version": "1.0.0",
  "description": "JavaScript/Typescript library for MLflow",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start-mlflow": "mlflow ui --port 5001",
    "test": "concurrently \"npm run start-mlflow\" \"wait-on http://localhost:5001 && jest\" --kill-others --success first",
    "posttest": "kill $(lsof -t -i:5001) || true",
    "lint": "eslint src",
    "docs": ""
  },
  "keywords": [
    "mlflow",
    "machine learning",
    "typescript",
    "ai",
    "data science"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.5.4",
    "concurrently": "^9.0.1",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "wait-on": "^8.0.1"
  },
  "engines": {
    "node": ">=22.7.0"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0"
  }
}
