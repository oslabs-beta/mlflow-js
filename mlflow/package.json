{
  "name": "mlflow-js",
  "version": "1.0.0",
  "description": "JavaScript library for MLflow",
  "homepage": "https://www.mlflow-js.org/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/mlflow-js.git"
  },
  "bugs": {
    "url": "https://github.com/oslabs-beta/mlflow-js/issues"
  },
  "pull_request": {
    "url": "https://github.com/oslabs-beta/mlflow-js/pulls"
  },
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "test": "jest",
    "lint": "eslint src",
    "docker": "docker-compose pull && docker-compose -f docker-compose.yml up",
    "dockerTest": "docker-compose pull && docker-compose -f docker-compose-test.yml up"
  },
  "keywords": [
    "mlflow",
    "mlops",
    "machine learning",
    "ai",
    "data science",
    "javascript",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.5.4",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  },
  "engines": {
    "node": ">=22.7.0"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0"
  }
}
